<p id="notice"><%= notice %></p>

<h1>Requests Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests_users.each do |requests_user| %>
      <tr>
        <td><%= link_to 'Show', requests_user %></td>
        <td><%= link_to 'Edit', edit_requests_user_path(requests_user) %></td>
        <td><%= link_to 'Destroy', requests_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Requests User', new_requests_user_path %>
